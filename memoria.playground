for x in 0...100 {
    
    if x % 10 == 5 || x % 10 == 0{
        print("\(x) bingo!")
    } else if x % 10 == 2 || x % 10 == 0 {
        print("\(x) es par")
    } else if x % 10 == 3 || x % 10 == 1 {
        print("\(x) es impar")
    }
    if x >= 30 && x <= 40 {
        print("\(x) viva Swift!")
    }
}
